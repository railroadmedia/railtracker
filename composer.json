{
  "name": "railroad/railtracker",
  "type": "library",
  "description": "Tracks user actions in your application.",
  "keywords": [
    "tracker",
    "pageview",
    "login",
    "tracking"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Caleb Favor",
      "email": "caleb@drumeo.com",
      "role": "Developer"
    }
  ],
  "prefer-stable": true,
  "require": {
    "php": "~7",
    "laravel/framework": "~5.6",
    "guzzlehttp/guzzle": "~6.0",
    "doctrine/dbal": "^2.5",
    "ua-parser/uap-php": "^3.4",
    "jenssegers/agent": "^2.5",
    "ramsey/uuid": "^3.6",
    "railroad/doctrine": "^0.0",
    "railroad/doctrine-array-hydrator": "^1.0",
    "ext-redis": "*",
    "predis/predis": "1.1.1",
    "fzaninotto/faker": "^1.8"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "orchestra/database": "~3.6",
    "orchestra/testbench": "~3.6"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Railtracker\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Railtracker\\Tests\\": "tests/"
    }
  }
}
