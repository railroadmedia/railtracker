{
  "name": "railroad/railtracker",
  "type": "library",
  "description": "Tracks user actions in your application.",
  "keywords": [
    "tracker",
    "pageview",
    "login",
    "tracking"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Caleb Favor",
      "email": "caleb@drumeo.com",
      "role": "Developer"
    }
  ],
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0",
    "guzzlehttp/guzzle": "^7.2",
    "doctrine/dbal": "^3.3",
    "jenssegers/agent": "^2.5",
    "ramsey/uuid": "^4.2.2",
    "predis/predis": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "phpunit/php-code-coverage": "^9.2.10",
    "orchestra/testbench": "^7.4"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Railtracker\\": "src"
    },
    "files": [
      "src/Helpers/railtracker_helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Railtracker\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true,
      "composer/package-versions-deprecated": true
    }
  }
}
